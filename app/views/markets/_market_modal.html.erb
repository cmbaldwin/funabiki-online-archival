<div class="modal fade" id="marketModal" tabindex="-1" role="dialog" aria-labelledby="marketModalLabel" aria-hidden="true" data-reload-path="<%= markets_index_markets_path %>"> 
  <div class="modal-dialog modal-xl">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="marketModalLabel">市場作成・編集<span id="supply_action_date_title"></span></h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="tab-content modal-body overflow-hidden" id="nav-tabContent">
        <div class="tab-pane fade show active" id="nav-partial" role="tabpanel">
          <%= turbo_frame_tag 'market_form' do %>
            <div class="d-flex justify-content-center">
              <div class="spinner-border" role="status">
                <span class="visually-hidden">読み込み中...</span>
              </div>
            </div> 
          <% end %>
        </div>
      </div>
    <div class="modal-footer">
      <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">閉じる</button>
    </div>
    </div>
  </div>
</div>